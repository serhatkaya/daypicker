{
  "name": "@serhatkaya/daypicker-core",
  "version": "0.0.0-rc.5",
  "description": "DaypickerComponent Core",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/daypicker-core/daypicker-core.esm.js",
  "exports": {
    ".": {
      "import": "./dist/daypicker-core/daypicker-core.esm.js",
      "require": "./dist/daypicker-core/daypicker-core.cjs.js"
    },
    "./sk-daypicker": {
      "import": "./dist/components/sk-daypicker.js",
      "types": "./dist/components/sk-daypicker.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serhatkaya/daypicker"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.8.4",
    "@stencil/core": "^4.17.2",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/vue-output-target": "^0.8.8",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "swiper": "^11.1.1"
  },
  "private": false
}
